@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/family.ecore>
        a       owl:Ontology .

<http://example.org/family.ecore#age>
        a            owl:DatatypeProperty ;
        rdfs:domain  <http://example.org/family.ecore#Person> .

<http://example.org/family.ecore#name>
        a            owl:DatatypeProperty ;
        rdfs:domain  <http://example.org/family.ecore#Person> .

<http://example.org/family.ecore#height>
        a            owl:DatatypeProperty ;
        rdfs:domain  <http://example.org/family.ecore#Person> .

<http://example.org/family.ecore#hasFather>
        a            owl:ObjectProperty ;
        rdfs:domain  <http://example.org/family.ecore#Person> ;
        rdfs:range   <http://example.org/family.ecore#Person> .

<http://example.org/family.ecore#Family>
        a                owl:Class ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "1"^^xsd:int ;
                           owl:onClass         <http://example.org/family.ecore#Person> ;
                           owl:onProperty      <http://example.org/family.ecore#member>
                         ] .

<http://example.org/family.ecore#hasUncle>
        a            owl:ObjectProperty ;
        rdfs:domain  <http://example.org/family.ecore#Person> ;
        rdfs:range   <http://example.org/family.ecore#Person> .

<http://example.org/family.ecore#Person>
        a                owl:Class ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:int ;
                           owl:onProperty   <http://example.org/family.ecore#age>
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:int ;
                           owl:onProperty   <http://example.org/family.ecore#height>
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:int ;
                           owl:onClass         <http://example.org/family.ecore#Person> ;
                           owl:onProperty      <http://example.org/family.ecore#hasFather>
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:int ;
                           owl:onProperty   <http://example.org/family.ecore#name>
                         ] .

<http://example.org/family.ecore#member>
        a            owl:ObjectProperty ;
        rdfs:domain  <http://example.org/family.ecore#Family> ;
        rdfs:range   <http://example.org/family.ecore#Person> .

<http://example.org/family.ecore#hasBrother>
        a            owl:ObjectProperty ;
        rdfs:domain  <http://example.org/family.ecore#Person> ;
        rdfs:range   <http://example.org/family.ecore#Person> .
